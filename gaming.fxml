<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<GridPane fx:id="rootGridPane" style="-fx-background-color: #F0ECEC;" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.abhishek.gametwo.Controller">
    <columnConstraints>
        <ColumnConstraints />
        <ColumnConstraints maxWidth="200.0" minWidth="176.0" prefWidth="176.0" />
    </columnConstraints>
    <rowConstraints>
        <RowConstraints minHeight="10.0" prefHeight="30.0" />
        <RowConstraints />
    </rowConstraints>
    <children>
        <Pane GridPane.columnSpan="2" />
        <Pane fx:id="insertedDiscsPane" prefHeight="400.0" prefWidth="200.0" GridPane.rowIndex="1" />
        <VBox style="-fx-background-color: #F9EBEB;" GridPane.columnIndex="1" GridPane.rowIndex="1">
            <children>
            <TextField fx:id="Playeronetextfield" prefHeight="36.0" prefWidth="176.0" promptText="PlayerOne Name">
               <VBox.margin>
                  <Insets left="15.0" right="15.0" top="35.0" />
               </VBox.margin></TextField>
            <TextField fx:id="Playertwotextfield" prefHeight="38.0" prefWidth="176.0" promptText="Player Two Name">
               <VBox.margin>
                  <Insets left="15.0" right="15.0" top="2.0" />
               </VBox.margin></TextField>
            <Button fx:id="SetNames" mnemonicParsing="false" prefHeight="25.0" prefWidth="177.0" text="SetName">
               <VBox.margin>
                  <Insets bottom="5.0" left="15.0" right="15.0" top="5.0" />
               </VBox.margin></Button>
                <Label fx:id="playerNameLabel" alignment="CENTER" contentDisplay="CENTER" prefHeight="61.0" prefWidth="176.0" text="Player One ">
                    <font>
                        <Font name="System Bold" size="29.0" />
                    </font>
               <VBox.margin>
                  <Insets top="70.0" />
               </VBox.margin>
                </Label>
                <Label alignment="CENTER" prefHeight="36.0" prefWidth="156.0" text="Turn">
                    <font>
                        <Font size="25.0" />
                    </font>
                </Label>
                <Region prefHeight="200.0" prefWidth="200.0" />
            </children>
        </VBox>
    </children>
</GridPane>
